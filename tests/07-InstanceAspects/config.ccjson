{
    "@": {
        "profile.impl": {
            "$": "./profile.ccjson"
        },
        "auth.impl": {
            "$": "./auth.ccjson"
        }
    },
    "@profile.impl": {
        "$profile": {
            "secret": "SecretValue"
        }
    },
    "@auth.impl": {
        "$auth": {
            "$profile.decrypt()->.": {
                "someVariable1": "(EncryptedUsing:SecretValue)Value1"
        	},            
            "$profile.decrypt()->set1": {
                "someVariable2": "(EncryptedUsing:SecretValue)Value2"
        	},
            "$profile.decrypt()->set2/sub": {
                "someVariable3": "(EncryptedUsing:SecretValue)Value3"
        	}
        }
    }
}